{
  "manifest_version": 2,
  "name": "FFockstish",
  "version": "1.0",

  "description": "Detect cheat-like behaviour in realtime on chess.com",

  "icons": {
    "48": "icons/icon_48x48.png",
    "96": "icons/icon_96x96.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.chess.com/*"],
      "js": ["analyze.js"]
    }
  ]
}
